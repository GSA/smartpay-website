---
// Component used for Top Navigation that return to parent nav page
import previousNavIcon from '@assets/images/previous-nav-icon.svg'
const { parent_nav_path, parent_nav_text} = Astro.props;
const url = `/${parent_nav_path}`
---
<div class="display-flex margin-bottom-2 width-full">
    <img src={previousNavIcon.src} aria-hidden="true" role="img" >
    <a class="usa-link" href={url}>{parent_nav_text}</a>
</div>